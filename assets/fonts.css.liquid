@font-face {
    font-family: 'marrthinitalic';
    src: url('{{ 'marrthinitalic.woff2' | asset_url }}') format('woff2'), url('{{ 'marrthinitalic.woff' | asset_url }}') format('woff');
    font-weight: 300;
    font-style: italic;
    font-display: swap; /* Read next point */  
    unicode-range: U+000-5FF; /* Download only latin glyphs */  
}

@font-face {
    font-family: 'marritalic';
    src: url('{{ 'marritalic.woff2' | asset_url }}') format('woff2'), url('{{ 'marritalic.woff' | asset_url }}') format('woff');
    font-weight: normal;
    font-style: italic;
    font-display: swap; /* Read next point */  
    unicode-range: U+000-5FF; /* Download only latin glyphs */  
}

@font-face {
    font-family: 'Druk Cond Super';
    src: url('{{ 'DrukCond-Super.woff2' | asset_url }}') format('woff2'), url('{{ 'DrukCond-Super.woff' | asset_url }}') format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap; /* Read next point */  
    unicode-range: U+000-5FF; /* Download only latin glyphs */  
}